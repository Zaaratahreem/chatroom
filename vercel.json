{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/routes.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}